package OOPS;

class vehicle {
	double price;
	double mileage;
	String color;

	void display() {
		System.out.println(price);
		System.out.println(mileage);
		System.out.println(color);
	}
}

class Car extends vehicle {
	String ftype;
	boolean sunroof;
	String brand;

}

public class OOPs4 {

	public static void main(String[] args) {
		Car car1 = new Car();
		car1.brand = "Tata";
		car1.price = 5000000;
		car1.mileage = 15.5;
		car1.ftype = "Diesle";
		car1.color = "Black";
		car1.sunroof = true;

		car1.display();
		System.out.println(car1.sunroof);
	}

}
