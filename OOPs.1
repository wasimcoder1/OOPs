package OOPS;

public class OOPs1 {
	
	String name;
	int roll_no;
	
	public void printdata() {
		System.out.println(name);
		System.out.println(roll_no);
	}

	public static void main(String[] args) {
		OOPs1 std1 = new OOPs1();
		std1.name = "Wasim";
		std1.roll_no = 15;
		System.out.println(std1.roll_no);
	    std1.printdata();
	    
	    
	    OOPs1 std2 = new OOPs1();
	    std2.name = "Ansari";
	    std2.roll_no = 13;
	    std2.printdata();
	}

}
